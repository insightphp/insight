{
    "name": "insightphp/inertia-view-components",
    "type": "library",
    "license": "MIT",
    "description": "View component middleware for Inertia.",
    "authors": [
        {
            "name": "Peter Stovka",
            "email": "peter@peterstovka.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "inertiajs/inertia-laravel": "^0.6",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Insight\\Inertia\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Insight\\Inertia\\Providers\\ViewComponentServiceProvider"
            ]
        }
    }
}
